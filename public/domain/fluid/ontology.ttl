###  http://www.bim4ren.eu/domain/fluid#HMTnoFlow
:HMTnoFlow rdf:type owl:ObjectProperty ;
           rdfs:subPropertyOf :totalDynamicHead .


###  http://www.bim4ren.eu/domain/fluid#HMTnominalFlow
:HMTnominalFlow rdf:type owl:ObjectProperty ;
                rdfs:subPropertyOf :totalDynamicHead .


###  http://www.bim4ren.eu/domain/fluid#capacity
:capacity rdf:type owl:ObjectProperty ;
          rdfs:subPropertyOf <http://www.bim4ren.eu/property#volume> ;
          rdfs:label "capacidad"@es ,
                     "capacity"@en ,
                     "capacité"@fr ,
                     "volume"@en ,
                     "volume"@fr ,
                     "volumen"@es .


###  http://www.bim4ren.eu/domain/fluid#maxPressure
:maxPressure rdf:type owl:ObjectProperty ;
             rdfs:subPropertyOf <http://www.bim4ren.eu/property#pressure> ;
             rdfs:label "massima pressione"@it ,
                        "maximal pressure"@en ,
                        "presión máxima"@es ,
                        "pression maximale"@fr .


###  http://www.bim4ren.eu/domain/fluid#minPressure
:minPressure rdf:type owl:ObjectProperty ;
             rdfs:subPropertyOf :pressure ;
             rdfs:label "minima pressione"@it ,
                        "minimal pressure"@en ,
                        "presión minima"@es ,
                        "pression minimale"@fr .


###  http://www.bim4ren.eu/domain/fluid#nominalFlowRate
:nominalFlowRate rdf:type owl:ObjectProperty ;
                 rdfs:subPropertyOf <http://www.bim4ren.eu/property#flowRate> ;
                 rdfs:label "caudal nominal"@es ,
                            "débit nominal"@fr ,
                            "nominal flow rate"@en ,
                            "portata nominale"@it .


###  http://www.bim4ren.eu/domain/fluid#pressure
:pressure rdf:type owl:ObjectProperty .


###  http://www.bim4ren.eu/domain/fluid#pressureDrop
:pressureDrop rdf:type owl:ObjectProperty ;
              rdfs:subPropertyOf <http://www.bim4ren.eu/property#pressure> ;
              rdfs:label "perdita di carico"@it ,
                         "perte de charge"@fr ,
                         "pressure drop"@en ,
                         "pérdida de carga"@es .


###  http://www.bim4ren.eu/domain/fluid#totalDynamicHead
:totalDynamicHead rdf:type owl:ObjectProperty ;
                  rdfs:subPropertyOf <http://www.bim4ren.eu/property#height> ;
                  rdfs:label "HMT"@fr ,
                             "Hauteur manométrique totale"@fr ,
                             "TDH"@en ,
                             "Total dynamic head"@en .


###  http://www.bim4ren.eu/property#flowRate
<http://www.bim4ren.eu/property#flowRate> rdf:type owl:ObjectProperty .


###  http://www.bim4ren.eu/property#height
<http://www.bim4ren.eu/property#height> rdf:type owl:ObjectProperty .


###  http://www.bim4ren.eu/property#pressure
<http://www.bim4ren.eu/property#pressure> rdf:type owl:ObjectProperty .


###  http://www.bim4ren.eu/property#volume
<http://www.bim4ren.eu/property#volume> rdf:type owl:ObjectProperty .


