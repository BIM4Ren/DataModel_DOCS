    <!-- http://www.bim4ren.eu/sot#ControlElement -->


    <owl:Class rdf:about="http://www.bim4ren.eu/sot#ControlElement">
        <rdfs:subClassOf rdf:resource="http://www.bim4ren.eu/sot#SystemComponent"/>
        <rdfs:comment xml:lang="en">A control element is used to control a network. Some examples are valves, actuators...</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.bim4ren.eu/sot#EmissionElement -->


    <owl:Class rdf:about="http://www.bim4ren.eu/sot#EmissionElement">
