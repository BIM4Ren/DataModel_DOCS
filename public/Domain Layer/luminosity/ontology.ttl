###  http://www.bim4ren.eu/domain/luminosity#reflection
:reflection rdf:type owl:ObjectProperty ;
            rdfs:subPropertyOf <http://www.bim4ren.eu/property#ratio> ;
            rdfs:label "R"@fr ,
                       "facteur de réflexion moyen"@fr ,
                       "reflection"@en ,
                       "reflexión"@es ,
                       "riflessione"@it ,
                       "réflexion"@fr .


###  http://www.bim4ren.eu/domain/luminosity#skyAngle
:skyAngle rdf:type owl:ObjectProperty .


###  http://www.bim4ren.eu/domain/luminosity#transmittance
:transmittance rdf:type owl:ObjectProperty ;
               rdfs:subPropertyOf <http://www.bim4ren.eu/property#ratio> ;
               rdfs:label "TL"@fr ,
                          "coefficient de transmission lumineuse"@fr ,
                          "transmittance"@en ,
                          "transmittance"@fr ,
                          "transmittancia"@es ,
                          "transmittanza"@it .


###  http://www.bim4ren.eu/domain/luminosity#visibleSkyAngle
:visibleSkyAngle rdf:type owl:ObjectProperty ;
                 rdfs:subPropertyOf <http://www.bim4ren.eu/property#angle> ;
                 rdfs:label "angle de ciel visible"@fr .


###  http://www.bim4ren.eu/property#angle
<http://www.bim4ren.eu/property#angle> rdf:type owl:ObjectProperty .


###  http://www.bim4ren.eu/property#ratio
<http://www.bim4ren.eu/property#ratio> rdf:type owl:ObjectProperty .


