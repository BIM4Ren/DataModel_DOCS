#################################################################

###  http://www.bim4ren.eu/domain/luminosity#OpaqueElement
:OpaqueElement rdf:type owl:Class ;
               rdfs:subClassOf owl:Thing ,
                               [ rdf:type owl:Restriction ;
                                 owl:onProperty :reflection ;
                                 owl:someValuesFrom <https://w3id.org/def/basicsemantics-owl#QuantityValue>
                               ] ;
               rdfs:label "elemento opaco"@es ,
                          "elemento opaco"@it ,
                          "opaque element"@en ,
                          "élément opaque"@fr .


###  http://www.bim4ren.eu/domain/luminosity#TransparentElement
:TransparentElement rdf:type owl:Class ;
                    rdfs:subClassOf owl:Thing ,
                                    [ rdf:type owl:Restriction ;
                                      owl:onProperty :skyAngle ;
                                      owl:someValuesFrom <https://w3id.org/def/basicsemantics-owl#QuantityValue>
                                    ] ,
                                    [ rdf:type owl:Restriction ;
                                      owl:onProperty :transmittance ;
                                      owl:someValuesFrom <https://w3id.org/def/basicsemantics-owl#QuantityValue>
                                    ] ;
                    rdfs:label "element transparent"@fr .


###  https://w3id.org/def/basicsemantics-owl#QuantityValue
<https://w3id.org/def/basicsemantics-owl#QuantityValue> rdf:type owl:Class .


